{
  "name": "feather-scroll",
  "scripts": {
    "build": "export NODE_ENV=production; ./node_modules/.bin/webpack -w",
    "lint": "NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "NODE_ENV=development npm run lint -- --fix"
  },
  "version": "2.0.0",
  "description": "a window infinite scroll container for react utilizing constant space complexity",
  "main": "./lib/feather-scroll.js",
  "repository": "https://github.com/garrettlr/featherScroll.git",
  "author": "Garrett Levy <garrettlr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
